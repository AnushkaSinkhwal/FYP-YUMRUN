{
  "name": "yumrun",
  "version": "1.0.0",
  "description": "YUMRUN - A health-focused food ordering and delivery web application",
  "main": "index.js",
  "scripts": {
    "install:all": "yarn install:backend && yarn install:frontend",
    "install:backend": "cd backend && yarn install",
    "install:frontend": "cd frontend && yarn install",
    "dev": "concurrently \"yarn run dev:backend\" \"yarn run dev:frontend\"",
    "dev:backend": "cd backend && yarn run dev",
    "dev:frontend": "cd frontend && yarn run dev",
    "build": "yarn run build:frontend",
    "build:frontend": "cd frontend && yarn run build"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/yourusername/yumrun.git"
  },
  "keywords": [
    "food-delivery",
    "health",
    "nutrition",
    "mern-stack"
  ],
  "author": "YumRun Team",
  "license": "MIT",
  "dependencies": {
    "concurrently": "^8.2.2",
    "dotenv": "^16.5.0",
    "mkdirp": "^3.0.1",
    "mongoose": "^8.13.2",
    "multer": "^1.4.5-lts.2"
  },
  "devDependencies": {
    "canvas": "^3.1.0"
  }
}
