    <script type="text/javascript">

        function myFunction(e, tab){   
            if(tab) { 
                document.getElementById(tab).click();
            }
			prev_dataset = e.previousElementSibling.dataset;
			document.getElementById('video-title-id').textContent = prev_dataset.title;
			document.getElementById('video-shortdesc-id').textContent = prev_dataset.shortdesc;
            document.querySelectorAll('.load-fund-media-content').forEach(function(wrapper) {
                wrapper.style.display = "block" 
            }) 
            document.querySelectorAll('.load-fund-video').forEach(function(wrapper) {
                wrapper.innerHTML = ""
                wrapper.style.display = "none"
            })  
        }

        function handlePlayVideo(event) { 
            var current =  event.currentTarget
            var next = current.nextElementSibling
            var video_link = next.dataset.link  

            if(!video_link) return 
            document.querySelectorAll('.load-fund-video').forEach(function(wrapper) {
                wrapper.innerHTML = ""
                wrapper.style.display = "none"
            })
            current.style.display = "none"
            next.style.display = "block"
            next.innerHTML = `
                                <iframe width="100%" height="400px" src="${video_link}?autoplay=1"></iframe>
                            `
        }
    </script> 
