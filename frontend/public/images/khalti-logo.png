<script>

   $(document).ready(function() {
       $('#login-form').submit(function(e) { // catch the form's submit event
            e.preventDefault()
            if(!isLoginValid()) return validateLogin()
            var btn = document.getElementById('khaltiSubmitBtn')
            var loader = document.getElementById('submitBtnLoader')
            btn.classList.add('disabled')
            loader.style.display = "inline-table"
			console.log($(this).serialize())
           $.ajax({ 
               data: $(this).serialize(), 
               type: $(this).attr('method'),
               url: $(this).attr('action'),
               success: function(response) {
                    btn.classList.remove('disabled')
                    loader.style.display = "none"
                    // $('#myModal').modal('hide');
                    // window.location.href = "/#/";
                    window.location.reload();
               },
               error: function(response) {
                    btn.classList.remove('disabled')
                    loader.style.display = "none"
                    var element = document.getElementById('loginError')
                    element.style.display = "block"
                    var err = JSON.parse(response.responseText)
                    var detail = err.detail
                    var tries = err.tries_remaining ? (` You have ${err.tries_remaining} remaining tries`) : ""
                    var msg = (detail + tries) || "Something went wrong"
                    element.innerHTML = msg
               }
           });
           return false;
       });
       
       $('#offer-slider').slick({
            dots: true,
            infinite: true,
            autoplay: true,
            autoplaySpeed: 3000,
       });

   });

   function isLoginValid() {
        var id = document.getElementById('khaltiId').value
        var password = document.getElementById('khaltiPassword').value
        if(id && password) return true
        return false
    }

    function validateLogin() {
        var id_dom = document.getElementById('khaltiId')
        var id = id_dom.value
        var password_dom = document.getElementById('khaltiPassword')
        var password = password_dom.value
        if(!id) {
            id_dom.classList.add("is-invalid")
        }
        if(!password) {
            password_dom.classList.add("is-invalid")
        }
        return true
    }

    function handleIdInput() {
        var dom = document.getElementById('khaltiId')
        var id = dom.value
        if(id) {
            dom.classList.remove("is-invalid")
        }
    }

    function handlePwdInput() {
        var dom = document.getElementById('khaltiPassword')
        var pwd = dom.value
        if(pwd) {
            dom.classList.remove("is-invalid")
        }
    }

    function pwdToggler() {
      var x = document.getElementById("khaltiPassword");
      if (x.type === "password") {
        x.type = "text";
      } else {
        x.type = "password";
      }
    }
</script>


